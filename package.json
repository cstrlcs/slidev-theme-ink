{
  "name": "slidev-theme-ink",
  "version": "1.0.2",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^52.6.0"
  },
  "devDependencies": {
    "@slidev/cli": "^52.6.0"
  },
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "routerMode": "hash",
      "canvasWidth": 1280,
      "fonts": {
        "sans": "Inter",
        "mono": "Fira Code"
      }
    }
  }
}